import "assets/apis/appm/app.api"

type (
	GetAppIndexReq {
		Id int64 `form:"id"`
	}
	GetAppIndexResp {
		Msg string `json:"msg"`
	}
)

@server(
	group: v1/index
)
service app-api {
	@doc (
		summary: "获取首页"
	)
	@server(
		handler: GetAppIndexHandler
	)
	get /app/index(GetAppIndexReq) returns (GetAppIndexResp)
}